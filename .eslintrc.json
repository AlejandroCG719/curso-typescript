{
    "env": {
        "es6": true
    },
    "settings": {
        "import/resolver": {
            "node": {
                "extendsions": [".js", ".ts"]
            }
    },
    "extends": [
        "airbnb-base",
        "plugin:@typescript-eslint/recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "@typescript-eslint/rule-name": "error",
        "arrow-parens": "off",
        "comma-dangle": "off",
        "spaced-comment": "off",
        "object-curly-newline": "off"
    }
    }
}